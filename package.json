{
	"name": "docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"build": "docusaurus build",
		"build:cf": "git fetch --unshallow && docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"crowdin": "crowdin",
		"translations:write": "yarn write-translations",
		"translations:upload": "yarn crowdin upload",
		"translations:download": "yarn crowdin download",
		"translations:sync": "yarn translations:write && yarn translations:upload && yarn translations:download",
		"typecheck": "tsc",
		"predocker:start": "yarn",
		"docker:start": "yarn start --host 0.0.0.0",
		"format": "prettier --write \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
		"lint": "echo \"No lint specified\" && exit 0",
		"lint:fix": "echo \"No lint specified\" && exit 0",
		"prettier:write": "prettier --write ./content",
		"prepublish:git": "npm run build",
		"publish:git": "release-it",
		"test": "echo \"No tests specified\" && exit 0",
		"test:watch": "echo \"No tests specified\" && exit 0",
		"test:cov": "echo \"No tests specified\" && exit 0",
		"test:debug": "echo \"No tests specified\" && exit 0",
		"test:ci": "echo \"No tests specified\" && exit 0"
	},
	"dependencies": {
		"@crowdin/cli": "^4.12.0",
		"@docusaurus/core": "3.9.2",
		"@docusaurus/plugin-google-tag-manager": "^3.9.2",
		"@docusaurus/plugin-pwa": "^3.9.2",
		"@docusaurus/preset-classic": "^3.9.2",
		"@docusaurus/remark-plugin-npm2yarn": "^3.9.2",
		"@mdx-js/react": "3.1.1",
		"@radix-ui/react-slot": "^1.2.4",
		"@tailwindcss/postcss": "^4.1.18",
		"babel-plugin-tsconfig-paths": "^1.0.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"docusaurus-plugin-sass": "0.2.6",
		"lucide-react": "^0.561.0",
		"motion": "^12.23.26",
		"next-themes": "^0.4.6",
		"prism-react-renderer": "2.4.1",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "^5.5.0",
		"react-syntax-highlighter": "^16.1.0",
		"sass": "1.96.0",
		"shiki": "^3.20.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.13",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@docusaurus/module-type-aliases": "^3.9.2",
		"@docusaurus/tsconfig": "^3.9.2",
		"@docusaurus/types": "^3.8.1",
		"@release-it/conventional-changelog": "^10.0.4",
		"@types/react-syntax-highlighter": "^15.5.13",
		"prettier": "^3.7.4",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"release-it": "^19.1.0",
		"typescript": "5.9.3"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"packageManager": "yarn@1.22.22"
}
